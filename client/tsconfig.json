{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015"],
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",  // or "es2020", "es2022", etc.
    },
    "include": ["src"]
}
  